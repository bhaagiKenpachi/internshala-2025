datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model Celebrity {
  id           String   @id @default(uuid())
  name         String
  category     String
  country      String
  instagramUrl String
  fanbaseCount Int
  createdAt    DateTime @default(now())
}
model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  role      String
  createdAt DateTime @default(now())
}
